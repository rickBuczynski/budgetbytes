<script>
try {
  setTimeout(function() {
    dataLayer.push({
      "event" : "gaEvent",
      "category" : "dwell",
      "action" : "1 minute elapsed",
      "label" : ""
    })
  }, 60000);  
} catch (e) {}
</script>


<script>
  $("footer.footer div.social-links li a").click(function()
  {
    try {
      var event_name = $(this).data("event");
      var location = $(this).data("location");
      dataLayer.push({
        "event": "gaEvent",
        "category": "share", 
        "action": event_name,
        "label": location
      });
    } catch (err) {}
  });

  var _ads_tracker_created = false;

  function create_ads_tracker()
  {
    if (_ads_tracker_created == true)
      return;

    try {
      ga('create', 'UA-962456-13', 'auto', { name: 'ads' } );
      _ads_tracker_created = true;
    } catch(err) {}
  }

  function adsTracking(num, type, campaign, action, dimension, value) {
    if (!_ads_tracker_created)
      create_ads_tracker();

    try {
      ga(num, type, campaign, action, dimension, value);
      ga('ads.send', 'pageview');
    } catch (e) { }
  };
</script>

  
    <script>
      var _topicsUrl = "http://www.seriouseats.com/topics/search";


$("section div.more").click(function(event) {
 
 event.preventDefault();
  
 var _parent = $(this.parentElement);
 var $more_button = $(this).find("a");
 
 var _postid = _parent.find("div.block-wrapper div.module").last().data("postid");

 var _postTagQuery = $more_button.data("tag-query");
 var _postindex = $more_button.data("index");
 var _postcount = $more_button.data("count");
 var _card = $more_button.data("card");
 var _show_author = $more_button.data("show-author");
 var _show_excerpt = $more_button.data("show-excerpt");
 var _show_kicker = $more_button.data("show-kicker");
 var _show_recipe_link = $more_button.data("show-recipe-link");

 var _fields = "id,permalink,thumbnail_300,thumbnail_625,thumbnail_1500,title,short_title";

 if (_show_excerpt == true)
   _fields += ",excerpt";
 if (_show_author == true)
   _fields += ",author_nickname";
 if (_show_recipe_link == true)
   _fields += ",recipes";
 if (_show_kicker == true)
   _fields += ",kicker";
 
 var _posts = _parent.find("div.block-wrapper:last");
 
 var topicsUrl = _topicsUrl + "?index=" + _postindex + "&count=" + _postcount + "&before=" + _postid + _postTagQuery + "&only=" + _fields;

 var newPosts = [];

 $.ajax({
   context: this,
   url: topicsUrl,
   dataType: 'jsonp',
   jsonp: 'jsonp',
   success: function(data, textStatus, jqXHR) {
     
     $.each(data["entries"], function(key, value) {
       var wrapper = $("<div>").addClass("module-wrapper");
       var container = $("<div>").addClass("module").attr("data-postid", value["id"]);
       
       var image = $("<img>").attr("src", value["thumbnail_625"]).attr("data-pin-no-hover", "true");

       if (_card == true)
       {
         container.addClass("card");
         if(value["thumbnail_1500"]) {
           image = $("<img>").attr("src", value["thumbnail_1500"]).attr("data-pin-no-hover", "true");
         }
       }

       var link = $("<a>").attr("href", value["permalink"]).append(image);
       var figure = $("<figure>").append(link);

       if(value["title"].indexOf("Easiest Summer Ever: ") >= 0)
       {
        value["title"] = value["title"].replace("Easiest Summer Ever: ", "")
       }

       var titleText = (value["short_title"] && value["short_title"].length > 0) ? value["short_title"] : value["title"];
       var textlink = $("<a>").attr("href", value["permalink"]).text(titleText);
       var title = $("<h4>").addClass("title").append(textlink);
       var meta = $("<div>").addClass("metadata").append(title);

       if (value["excerpt"] != undefined)
       {
         var excerpt = $("<p>").addClass("excerpt").html(value["excerpt"]);
         meta.append(excerpt);
       }
       else if (value["kicker"] != undefined)
       {
         var excerpt = $("<p>").addClass("excerpt").html(value["kicker"]);
         meta.append(excerpt);
       }

       var footer = $("<footer>");
       if (_show_recipe_link && value["recipes"] != undefined && value["recipes"].length > 0)
       {
          var recipe_container = $("<div>").addClass("callout callout-recipe");
          var recipe_title = $("<h6>").addClass("callout-title");
          var recipe_icon_wrapper = $("<span>").addClass("icon-wrapper");
          var recipe_icon = $("<i>").addClass("icon icon-category-recipes");

          if (value["recipes"].length == 1)
          {
            recipe_container.addClass("callout-recipe-single");
            recipe_title.text("Recipe");
          }
          else if (value["recipes"].length > 0)
          {
            recipe_title.text("Recipes");
          }

          var recipe_block = recipe_container.append(recipe_title.prepend(recipe_icon_wrapper.prepend(recipe_icon)));

          var recipe_list = $("<ul>");
          for (var i = 0; i < value["recipes"].length; ++i)
          {
            var recipe = value["recipes"][i];
            var recipe_link = $("<a>").addClass("callout-recipe-link").text(recipe.title);
            var recipe_item = $("<li>").append(recipe_link);
            recipe_list.append(recipe_item);
          }

          footer.append(recipe_block.append(recipe_list));
       }


       if (value["author_nickname"] != undefined)
       {
         var author = $("<span>").addClass("author").html(value["author_nickname"]);
         footer.append(author);
       }

       meta.append(footer);
       wrapper.append(figure).append(meta);
       container.append(wrapper);
       newPosts.push(container[0]);
     });
   },
   complete: function() { 
     
     if (newPosts.length > 0)
     {
       _posts.append(newPosts);
     }
     if (newPosts.length < _postcount)
       $(this).find("a").addClass("btn-disabled").text("Sorry, We're all out!");
   }
 });
    
});

  $(document).ready(function() {
  });

    </script>

  
</body></html>