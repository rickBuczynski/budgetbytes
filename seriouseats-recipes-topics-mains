<!-- Yieldbot.com Intent Tag LOADING  -->
<script type="text/javascript">
  var ybotq = ybotq || [];
  (function() {
    var js = document.createElement('script');
    js.src = '//cdn.yldbt.com/js/yieldbot.intent.js';
    var node = document.getElementsByTagName('script')[0];
    node.parentNode.insertBefore(js, node);
  })();
</script>

<script type="text/javascript">

  var _validTags = ['@pomi', '@hellofresh', '@lavatools', '@toyota', '@thrivemarket', '@kingsford', '@blueberrycouncil', '@oscarmayer', '@soyvay'];

var _adSlots = new Array();
//array to support refreshes
var yb_slots = [];

$(document).ready(function() {
  
  <!-- Yieldbot.com Intent Tag Activation -->
  ybotq.push(function () {

    var yieldbot_window_width = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
    if (yieldbot_window_width <= 480)
      yieldbot.pub('83f4');
    else
      yieldbot.pub('92ce');

    // Only apply to standard above-the-fold placements
    var ads = $("body div.auto-ad#homepage-top-billboard,#entry-top-billboard,#sidebar-top").filter(":visible");
    for (i = 0; i < ads.length; i++)
    {
      var ad = $(ads[i]);
      var adUnit = ad.data("ad-unit");
      var adSize = ad.data("ad-size");

      if (!adUnit.length || !adSize.length)
        continue;

      yieldbot.defineSlot(adUnit, {sizes: adSize});
    }

    yieldbot.enableAsync();
    yieldbot.go();
  });

  var ads = $("body div.auto-ad").filter(":visible");
  var dfpKeyCount = 0;
  var maxKeys = 10;
  var index = 0;
  
  if (ads.length)
  {
    ybotq.push(function() {
      googletag.cmd.push(function() {
      


        googletag.pubads().setTargeting("Section", "category");
        dfpKeyCount++;



      
      // leaderboard and billboard mapping
      var horizontal_mapping = googletag.sizeMapping()
        .addSize([1024,0], [[970,250], [728,90], [970,90]])
        .addSize([768,0], [728,90])
        .addSize([0,0], [320,50])
        .build();

      var horizontal_short_mapping = googletag.sizeMapping()
        .addSize([1024,0], [[728,90], [970,90]])
        .addSize([768,0], [728,90])
        .addSize([0,0], [320,50])
        .build();

      var horizontal_no_mobile_mapping = googletag.sizeMapping()
        .addSize([0,0], [])
        .addSize([768,0], [728,90])
        .addSize([1024,0], [[970,250], [728,90], [970,90]])
        .build();

      var horizontal_narrow_no_mobile_mapping = googletag.sizeMapping()
        .addSize([0,0], [])
        .addSize([768,0], [728,90])
        .addSize([1464,0], [[970,250], [728,90], [970,90]])
        .build();

      var below_entry_mapping = googletag.sizeMapping()
        .addSize([972,0], [])
        .addSize([768,0], [300,250])
        .addSize([0,0], [320,50])
        .build();

      var vertical_mapping = googletag.sizeMapping()
        .addSize([1464, 0], [[300,250],[300,600]])
        .addSize([1080, 0], [[300,250],[336,280],[300,600]])
        .addSize([0, 0], [[300,250],[300,600]])
        .build();

      var vertical_600_mapping = googletag.sizeMapping()
        .addSize([972, 0], [300,600])
        .addSize([0, 0], []);

      var vertical_250_mapping = googletag.sizeMapping()
        .addSize([972, 0], [300,250])
        .addSize([0, 0], []);

      var logo_mapping = googletag.sizeMapping()
        .addSize([0,0], [[120,100], [120,185]])
        .build();
        
      var mobile_mapping = googletag.sizeMapping()
        .addSize([768,0], [])
        .addSize([0,0], [300,250])
        .build();

      var flavorprint_mapping = googletag.sizeMapping()
        .addSize([1464,0], [970,250])
        .addSize([972, 0], [600,250])
        .addSize([0,0], [300,250])
        .build();

      for (i = 0; i < ads.length; i++)
      {
        var ad = ads[i];
        var unit = $(ad).data("ad-unit");
        var size = $(ad).data("ad-size");
        var require_tag_match = $(ad).data("require-tag-match");
        var id = ad.id;

        var out_of_page = false;
        try {
          out_of_page = $(ad).data("ad-oop");
        } catch (err) {}
        
        if (!unit.length || !size.length || !id.length)
          continue;
        
        var validTags = [];
        if ($(ad).data("ad-tag"))
        {
          var tags = $(ad).data("ad-tag").replace(/\s/g, '').toLowerCase().split(",");
          for (var j = 0; j < tags.length; ++j)
          {
            if (_validTags.indexOf(tags[j]) >= 0)
              validTags.push(tags[j]);
          }

          if (require_tag_match && validTags.length == 0)
            continue;
        }

        ad["no_display"] = false;
        try {
          if ($(ad).data("no-display") == true)
            ad["no_display"] = true;
        } catch (err) {}
          
        if (out_of_page == true)
        {
          _adSlots[index] = googletag.defineOutOfPageSlot('/11927475/' + unit, id).addService(googletag.pubads()).setCollapseEmptyDiv(true,true);
          ad["defined"] = true;
        }
        else
        {
          _adSlots[index] = googletag.defineSlot("/11927475/" + unit, size, id).addService(googletag.pubads()).setCollapseEmptyDiv(true,true);
          ad["defined"] = true;
        }
        
        try {
          var ad_mapping = $(ad).data("ad-mapping");
          if (ad_mapping == "horizontal_mapping")
            _adSlots[index].defineSizeMapping(horizontal_mapping);
          else if (ad_mapping == "horizontal_short_mapping")
            _adSlots[index].defineSizeMapping(horizontal_short_mapping);
          else if (ad_mapping == "horizontal_no_mobile_mapping")
            _adSlots[index].defineSizeMapping(horizontal_no_mobile_mapping);
          else if (ad_mapping == "horizontal_narrow_no_mobile_mapping")
            _adSlots[index].defineSizeMapping(horizontal_narrow_no_mobile_mapping);
          else if (ad_mapping == "vertical_mapping")
            _adSlots[index].defineSizeMapping(vertical_mapping);
          else if (ad_mapping == "vertical_600_mapping")
            _adSlots[index].defineSizeMapping(vertical_600_mapping);
          else if (ad_mapping == "vertical_250_mapping")
            _adSlots[index].defineSizeMapping(vertical_250_mapping);
          else if (ad_mapping == "logo_mapping")
            _adSlots[index].defineSizeMapping(logo_mapping);
          else if (ad_mapping == "below_entry_mapping")
            _adSlots[index].defineSizeMapping(below_entry_mapping);
          else if (ad_mapping == "mobile_mapping")
            _adSlots[index].defineSizeMapping(mobile_mapping);
          else if (ad_mapping == "flavorprint_mapping")
            _adSlots[index].defineSizeMapping(flavorprint_mapping);
        } catch (err) {}

        // Only apply to standard above-the-fold placements
        if ($.inArray(ad.id, ["homepage-top-billboard", "entry-top-billboard", "sidebar-top"]) >= 0)
        {
          yieldbot.setSlotTargeting(unit, _adSlots[index]);
          yb_slots.push({'slot_name': unit, 'slot_var':_adSlots[index]});
        }  

        if ($(ad).data("ad-block") && dfpKeyCount < maxKeys)
        {
          _adSlots[index].setTargeting("Block", $(ad).data("ad-block"));
          dfpKeyCount++;
        }

        if (dfpKeyCount < maxKeys)
        {
          if (require_tag_match == true)
          {
            if (validTags.length > 0)
            {
              _adSlots[index].setTargeting("Tag", validTags.join(",").substring(0,40));
              dfpKeyCount++;
            }
          }
          else if ($(ad).data("ad-tag"))
          {
            _adSlots[index].setTargeting("Tag", $(ad).data("ad-tag").replace(/\s/g, '').substring(0,40));
            dfpKeyCount++;
          }            
        }
        index++;
      }

      googletag.pubads().enableSingleRequest();
    });

    googletag.enableServices();    
    
    for (var k = 0; k < ads.length; ++k)
    {
      var ad = ads[k];
      if (ad["no_display"] == true)
        continue;
      if (ad["defined"] == true)
        googletag.display(ad.id);
    }
  });
  }
});

</script>



    
  
<script type="text/javascript">
  (function() {
    var config = {
      kitId: 'aaq6fvb',
    };
    var d = false;
    var tk = document.createElement('script');
    tk.src = '//use.typekit.net/' + config.kitId + '.js';
    tk.type = 'text/javascript';
    tk.async = 'true';
    tk.onload = tk.onreadystatechange = function() {
      var rs = this.readyState;
      if (d || rs && rs != 'complete' && rs != 'loaded') return;
      d = true;
      try { Typekit.load(config); } catch (e) {}
    };
    var s = document.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(tk, s);
  })();
</script>

